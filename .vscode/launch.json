// This configuration is for debugging a web application using Node.js in Visual Studio Code.
{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Web App",
      "cwd": "${workspaceFolder}/apps/web",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "dev"]
    }
  ]
}